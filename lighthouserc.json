{
  "ci": {
    "collect": {
      "url": [
        "https://36exps.github.io/digitaler-fahrplan/"
      ],
      "settings": {
        "preset": "mobile",
        "throttlingMethod": "devtools"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "resource-summary:script": [
          "error",
          {
            "maxNumericValue": 102400
          }
        ]
      }
    }
  }
}